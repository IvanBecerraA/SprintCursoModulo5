<!-- Campos adicionales para Administrativo -->
<div id="administrativo" class="cantainer" style="display: none;">

		    <h2 class="pb-4">Datos del usuario Administrativo</h2>

    <div class="form-floating mb-3">
        <label for="area">Área</label>
        <input
            type="text"
            id="area"
            class="form-control"
            name="area"
            placeholder="RRHH"
            required
        >
    </div>
    <div class="form-floating mb-3">
        <label for="experienciaPrevia">Experiencia Previa</label>
        <input
            type="text"
            id="experienciaPrevia"
            class="form-control"
            name="area"
            placeholder="4 años"
            required
        >
    </div>
</div>